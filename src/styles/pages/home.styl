.ca-eagles {
  height: 100%;

  section {
     margin-top: 20px !important;

    @media only screen and (min-width: 600px) {
      margin-top: 30px !important;
    } 
  }

  .content-wrapper {
    position: relative;
    min-height: 82vh;
    background-image: url('../assets/bg.png');
    background-size: cover;
    top: 80px;
    padding-top: 20px;
    margin-bottom: 80px;

    @media only screen and (min-width: 600px) {
      top: 100px;
      margin-bottom: 100px;
    } 
  }

  .content {
    padding: 10px;
    max-width: 1230px;
    margin: auto;

    section {
      margin: 40px 0;
    }
  }
}